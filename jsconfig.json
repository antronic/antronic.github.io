{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "@p5/*": [
        "src/p5/*"
      ],
      "@comps/*": [
        "src/components/*"
      ],
      "@ducks/*": [
        "src/ducks/*"
      ],
      "@vars/*": [
        "src/vars/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}